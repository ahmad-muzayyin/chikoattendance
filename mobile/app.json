{
    "expo": {
        "name": "ChikoAttendance",
        "slug": "chiko-attendance",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/logo.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#2563EB"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.chiko.attendance"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo.png",
                "backgroundColor": "#2563EB"
            },
            "package": "com.chiko.attendance"
        },
        "web": {
            "favicon": "./assets/logo.png"
        },
        "plugins": [],
        "newArchEnabled": true
    }
}